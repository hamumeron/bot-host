  "dependencies": {
    "express": "^4.18.2",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "vite": "^4.3.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
